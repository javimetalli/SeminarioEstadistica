
public class Variables {

	public static void main(String[] args) {
		
		int edad = 35; //declarando la variable edad de tipo entero e inicializando con 35
		
		edad=75;
		/*
		 * la siguiente instrucción imprime en la consola(System.out) el valor de la variable edad
		 */
		System.out.println(edad); //al objeto out(pantalla) del sistema se esta iniciado el metodo prinln
	}
	

}


